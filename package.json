    "sequence",
    "promise",
    "synchronize"
  ],
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/czajkowski/sequentialize.git"
  },
  "author": "Piotr Czajkowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/czajkowski/sequentialize/issues"
  },
  "homepage": "https://github.com/czajkowski/sequentialize#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "jest": "^22.4.3"
  },
  "jest": {
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "collectCoverage": true,
    "testURL": "http://localhost"
  }
}
